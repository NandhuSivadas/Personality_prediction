{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "includeFiles": ["model.joblib", "questions.json"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}